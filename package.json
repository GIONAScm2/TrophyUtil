{
	"name": "trophyutil",
	"description": "Isomorphic library of specialized trophy utilities",
	"author": {
		"name": "Matthew Gionas",
		"url": "https://github.com/GIONAScm2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GIONAScm2/TrophyUtil"
	},
	"keywords": [
		"PSN",
		"PSNProfiles",
		"Trophy hunting"
	],
	"version": "7.3.0",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:stacks": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/models/stackData.test",
		"build": "tsc && tsc -p tsconfig.cjs.json",
		"start": "ts-node-esm src/index.ts",
		"start:prod": "npm run build && node dist/index.js"
	},
	"license": "GPL-3.0-or-later",
	"devDependencies": {
		"@inrupt/jest-jsdom-polyfills": "^1.6.2",
		"@types/jest": "^29.5.1",
		"@types/jsdom": "^21.1.1",
		"@types/node": "^18.15.11",
		"enhanced-resolve": "^5.12.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jsdom": "^21.1.1",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}
